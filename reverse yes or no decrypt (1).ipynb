    "#reverse decryption\n",
    "a=input(\"enter the message: \")\n",
    "b=input(\"enter yes or no: \")\n",
    "def reverse(a,b):\n",
    "    if(b==\"yes\"):\n",
    "        c=list(a)\n",
    "        d=reversed(c)\n",
    "        e=\"\".join(d)\n",
    "        return e\n",
    "    else:\n",
    "        return a\n",
    "reverse(a,b)"
   ]
